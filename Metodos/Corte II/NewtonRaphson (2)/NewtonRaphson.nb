Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"NewtonRaphson", "[", 
    RowBox[{"p0_", ",", "cifras_"}], "]"}], ":=", 
   RowBox[{"Method", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", 
     RowBox[{
      RowBox[{"a", "=", 
       RowBox[{"N", "[", "p0", "]"}]}], ";", 
      RowBox[{"e", "=", 
       SuperscriptBox["10", 
        RowBox[{"-", 
         RowBox[{"N", "[", "cifras", "]"}]}]]}], ";", 
      RowBox[{"n", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"b", "=", 
       RowBox[{"a", "-", 
        RowBox[{"N", "[", 
         FractionBox[
          RowBox[{"f", "[", "a", "]"}], 
          RowBox[{
           RowBox[{"f", "'"}], "[", "a", "]"}]], "]"}]}]}], ";", 
      RowBox[{"error", "=", 
       RowBox[{"Abs", "[", 
        FractionBox[
         RowBox[{"b", "-", "a"}], "b"], "]"}]}], ";", 
      RowBox[{"vector", "=", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"n", ",", "a", ",", "b", ",", 
          RowBox[{"f", "[", "b", "]"}], ",", "error"}], "}"}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"f", 
               RowBox[{"(", "b", ")"}]}], ">", "e"}], ")"}], "||", 
            RowBox[{"(", 
             RowBox[{"error", "\[GreaterEqual]", " ", 
              RowBox[{"5", "*", "e"}]}], ")"}]}], ")"}], "&&", 
          RowBox[{"n", "\[LessEqual]", "20"}]}], ")"}], ",", 
        RowBox[{
         RowBox[{"a", "=", "b"}], ";", 
         RowBox[{"b", "=", 
          RowBox[{"a", "-", 
           FractionBox[
            RowBox[{"f", "[", "a", "]"}], 
            RowBox[{
             RowBox[{"f", "'"}], "[", "a", "]"}]]}]}], ";", 
         RowBox[{"error", "=", 
          RowBox[{"Abs", "[", 
           FractionBox[
            RowBox[{"b", "-", "a"}], "b"], "]"}]}], ";", 
         RowBox[{"n", "=", 
          RowBox[{"n", "+", "1"}]}], ";", " ", 
         RowBox[{"vector", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"vector", ",", 
            RowBox[{"{", 
             RowBox[{"n", ",", "a", ",", "b", ",", 
              RowBox[{"f", "[", "b", "]"}], ",", "error"}], "}"}]}], 
           "]"}]}]}]}], "\[IndentingNewLine]", "]"}], ";", 
      RowBox[{"Print", "[", 
       RowBox[{"NumberForm", "[", 
